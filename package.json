{
  "name": "get-tsconfig-compat",
  "version": "0.0.1",
  "description": "Portable version of get-tsconfig for earlier versions of node.js",
  "keywords": [
    "tsconfig.json",
    "tsconfig",
    "get-tsconfig"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/get-tsconfig-compat.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "",
    "build:get-tsconfig": "rollup -c config/rollup.gts.config.js",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint && depcheck",
    "test": "mocha-compat test/**/*.test.*"
  },
  "dependencies": {
    "core-js": "^3.24.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/parser": "^5.32.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "get-tsconfig": "^4.2.0",
    "mocha-compat": "^3.5.5",
    "prettier": "^2.7.1",
    "rollup": "^2.77.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-externals": "^4.1.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=0.8"
  }
}

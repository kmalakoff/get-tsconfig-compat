{
  "name": "get-tsconfig-compat",
  "version": "1.0.1",
  "description": "Portable version of get-tsconfig for earlier versions of node.js",
  "keywords": [
    "tsconfig.json",
    "tsconfig",
    "get-tsconfig"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/get-tsconfig-compat.git"
  },
  "license": "MIT",
  "type": "commonjs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "format": "",
    "build": "",
    "build:tsconfig": "rollup -c config/rollup.gts.config.mjs && echo \"export * from 'get-tsconfig';\" > dist/index.d.ts",
    "prepublishOnly": "depcheck",
    "test": "mocha-compat test/**/*.test.*",
    "test:engines": "nvu engines npm test"
  },
  "dependencies": {
    "core-js": "^3.39.0",
    "get-tsconfig": "*"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@types/node": "^22.10.1",
    "depcheck": "^1.4.7",
    "mocha-compat": "^3.6.2",
    "rollup": "^4.28.1",
    "rollup-plugin-node-externals": "^7.1.3",
    "slash": "^1.0.0",
    "ts-swc-rollup-plugin": "^1.1.1",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=0.8"
  }
}
